{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "removeComments": false,
    "sourceMap": true,
    "target": "es2023",
    "outDir": "dist",
    "declaration": true,
    "strict": true,
    "module": "es2022",
    "lib": [
      "es2023"
